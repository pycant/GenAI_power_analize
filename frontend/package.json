{
  "name": "genai-power-evaluation-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "GenAI模型能效评级体系前端应用",
  "author": "GenAI模型能效评级体系项目组",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/genai-power-evaluation"
  },
  "bugs": {
    "url": "https://github.com/your-org/genai-power-evaluation/issues"
  },
  "homepage": "https://github.com/your-org/genai-power-evaluation#readme",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "build": "tsc && vite build",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "vite-bundle-analyzer dist",
    "clean": "rimraf dist node_modules/.cache",
    "pre-commit": "lint-staged",
    "prepare": "cd .. && husky install frontend/.husky",
    "generate:component": "plop component",
    "generate:page": "plop page",
    "generate:hook": "plop hook",
    "generate:api": "plop api",
    "postinstall": "patch-package"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
